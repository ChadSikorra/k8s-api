{
    "name": "k8s/api",
    "description": "Kubernetes API models and services generated from the OpenAPI spec.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chad Sikorra",
            "email": "Chad.Sikorra@gmail.com"
        }
    ],
    "repositories": [
        { "type": "vcs", "url": "https://github.com/ChadSikorra/k8s-api-generator.git" },
        { "type": "vcs", "url": "https://github.com/ChadSikorra/k8s-core.git" }
    ],
    "require": {
        "php": ">=7.2",
        "k8s/core": "^1.0"
    },
    "require-dev": {
        "k8s/api-generator": "^0.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "K8s\\Api\\": "src/K8s/Api"
        }
    }
}
